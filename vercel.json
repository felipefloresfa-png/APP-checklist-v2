{
  "rewrites": [
    { "source": "/(.*\\.tsx)", "destination": "/$1.tsx" },
    { "source": "/(.*\\.ts)", "destination": "/$1.ts" }
  ],
  "headers": [
    {
      "source": "/(.*\\.ts)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*\\.tsx)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
}