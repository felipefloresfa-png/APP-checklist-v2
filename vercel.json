{
  "version": 2,
  "rewrites": [
    { "source": "^/(.+\\.tsx)$", "destination": "/$1" },
    { "source": "^/(.+\\.ts)$", "destination": "/$1" }
  ],
  "headers": [
    {
      "source": "/.+\\.(ts|tsx)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
}