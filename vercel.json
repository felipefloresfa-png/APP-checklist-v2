{
  "framework": null,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.tsx)",
      "headers": [
        { "key": "Content-Type", "value": "text/javascript" }
      ]
    },
    {
      "source": "/(.*\\.ts)",
      "headers": [
        { "key": "Content-Type", "value": "text/javascript" }
      ]
    }
  ]
}