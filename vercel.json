{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "index.tsx", "use": "@vercel/static" }
  ],
  "headers": [
    {
      "source": "/index.tsx",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
}